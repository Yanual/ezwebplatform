<mymw:document xmlns:mymw="http://morfeo-project.org/mymobileweb" id="plain">
  <mymw:head>
    <mymw:title>Error</mymw:title>
  </mymw:head>
  <mymw:body>
    <mymw:p id="p1" class="center vertical">
      <mymw:image id="ierror" resourceid="_MYMW_ERROR" alt="Error"></mymw:image>
    </mymw:p>
    <mymw:p id="p2" class="center vertical">
      <mymw:label class="small">${_MYMW_USER_MSG}</mymw:label>
      <mymw:submit id="_MYMW_E_BACK" class="novalidate accept" value="Back"></mymw:submit>
    </mymw:p>
  </mymw:body>
</mymw:document>

